{
  "name": "bookmarked-types",
  "version": "1.0.0",
  "description": "Shared TypeScript types and Zod schemas for Bookmarked application",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc",
