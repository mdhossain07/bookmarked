{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ],
  "functions": {
    "api/index.ts": {
      "maxDuration": 10,
      "includeFiles": [
        "../packages/bookmarked-types/dist/**",
        "../packages/bookmarked-types/package.json"
      ]
    }
  }
}
